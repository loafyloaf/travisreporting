{
  "name": "natural-language-understanding-starter",
  "version": "0.0.1",
  "description": "A sample Node.js app for Bluemix that use the IBM Watson Natural Language Understanding service",
  "private": true,
  "engines": {
    "node": ">=4.0",
    "npm": ">=3.0"
  },
  "scripts": {
    "start": "node server/server.js",
    "debug": "chmod +x run-debug && ./run-debug",
    "test": "nyc mocha",
    "dev": "nodemon server/server.js",
    "postinstall": "npm run build",
    "build": "./node_modules/webpack/bin/webpack.js"
  },
  "nodemonConfig": {
    "env": {
      "NODE_HEAPDUMP_OPTIONS": "nosignal"
    }
  },
  "dependencies": {
    "appmetrics-dash": "^3.3.2",
    "appmetrics-prometheus": "^0.0.2",
    "appmetrics-zipkin": "^1.0.4",
    "body-parser": "^1.17.2",
    "express": "~4.15.3",
    "log4js": "^1.1.1",
    "ibm-cloud-env": "^0",
    "watson-developer-cloud": "^3.4.1",
    "aphrodite": "^1.2.3",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "css-math": "^0.4.0",
    "deep-assign": "^2.0.0",
    "html-to-react": "^1.2.11",
    "language-list": "0.0.3",
    "map-range": "^0.1.2",
    "numeral": "^2.0.6",
    "prop-types": "^15.5.10",
    "react-dom": "^15.6.1",
    "react-waypoint": "^7.0.4",
    "react": "^15.6.1",
    "scroll-to-element": "^2.0.0",
    "style-loader": "^0.18.2",
    "valid-url": "^1.0.9",
    "vcap_services": "^0.3.4",
    "watson-react-components": "^0.6.9",
    "webpack": "^3.5.4",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "nodemon": "^1.12.1",
    "chai": "^4.0.0",
    "mocha": "^3.4.2",
    "nyc": "^10.3.2"
  },
  "author": "IBM Corp.",
  "contributors": [
    {
      "name": "Mamoon Raja",
      "email": "mamoon.raja@ibm.com",
      "url": "https://github.ibm.com/mamoon-raja/"
    },
    {
      "name": "German Attanasio",
      "email": "germanatt@us.ibm.com",
